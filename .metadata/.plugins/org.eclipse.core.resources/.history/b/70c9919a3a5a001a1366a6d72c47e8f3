package com.project.helpers;

public class RandomUtil {

	
	public static String getRandomID(int from,int to) {
		for(int i =0; i<3; i++){
		      int randomInt = (int)(10.0 * Math.random());
		      System.out.println("pseudo random int between 1 and 10 : " + randomInt );
		}


		Read more: https://javarevisited.blogspot.com/2013/05/how-to-generate-random-numbers-in-java-between-range.html#ixzz6FGEmA1UG
	}
	
}

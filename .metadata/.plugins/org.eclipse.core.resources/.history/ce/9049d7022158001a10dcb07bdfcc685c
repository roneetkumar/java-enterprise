package com.college.model;

import java.util.ArrayList;

public class Student {
	private String name;
	private int age;
	private String email;
	private ArrayList<Course> courses;
	
	
	public Student(String name, int age, String email, ArrayList<Course> courses) {
		super();
		this.name = name;
		this.age = age;
		this.email = email;
		this.courses = courses;
	}
	
	
}
